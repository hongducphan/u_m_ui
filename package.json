    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "chart.js": "^2.7.3",
    "classnames": "^2.2.6",
    "core-js": "^2.5.7",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "flag-icon-css": "^3.2.1",
    "font-awesome": "^4.7.0",
    "jwt-decode": "^2.2.0",
    "node-sass": "^4.10.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.1",
    "react-app-polyfill": "^0.1.3",
    "react-avatar-uploader": "^1.0.3",
    "react-awesome-modal": "^2.0.5",
    "react-chartjs-2": "^2.7.2",
    "react-contenteditable": "^3.2.2",
    "react-dom": "^16.6.1",
    "react-loadable": "^5.5.0",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.6.1",
    "reactstrap": "^6.5.0",
    "simple-line-icons": "^2.4.1",
    "sinon": "^7.1.1"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.2.0",
    "jest-junit": "^5.2.0",
    "prettier": "1.15.2",
    "pretty-quick": "^1.8.0",
    "react-scripts": "^2.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "coverage": "react-scripts test --coverage",
    "precommit": "pretty-quick --staged",
    "format-code": "prettier --write src/*.js src/**/*.js",
    "test:debug": "react-scripts --inspect-brk test --runInBand",
    "test:ci": "react-scripts test --env=jsdom --reporters=default --reporters=jest-junit --coverage --coverageReporters=cobertura && eslint -f json -o eslint-report.json src/*.js src/**/*.js",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended",
      "prettier/react",
      "prettier/standard"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 9",
    "not op_mini all"
  ]
}
